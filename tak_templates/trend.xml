<?xml version="1.0" encoding="UTF-8"?>
<trend id="{ID}" name="{TAK_NAME}" concept-type="trend" significant-variation="{SIGNIFICANT_VARIATION}">
  <categories/>
  <standard-terms/>
  <synonyms/>
  <temporal-semantic downward-hereditary="true" forward="true" backward="true" solid="true" concatenable="true" gestalt="true"/>
  <clippers/>
  
  <derived-from>
    <derived-from-id>{DERIVED_FROM}</derived-from-id>
  </derived-from>
  
  <abstraction-at-contexts/>

  <gradient-trend-allowed-values output-type="ordinal">
    <persistence>
      <global-persistence granularity="second">
        <interpolation-table>
          <rows/>
        </interpolation-table>
      </global-persistence>
      <local-persistence>
        <good-before value="0" granularity="second"/>
        <good-after value="0" granularity="second"/>
      </local-persistence>
    </persistence>
    <values>
      <ordinal-allowed-value order="0" value="DEC"/>
      <ordinal-allowed-value order="1" value="SAME"/>
      <ordinal-allowed-value order="2" value="INC"/>
    </values>
  </gradient-trend-allowed-values>

  <time-steady value="{TIME_STEADY_VALUE}" granularity="{TIME_STEADY_UNIT}"/>
</trend>